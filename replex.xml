<?xml version="1.0"?>
<Container version="2">
  <Name>RePlex</Name>
  <Repository>klippel/replex</Repository>
  <Registry>https://hub.docker.com/r/klippel/replex/</Registry>
  <Network>bridge</Network>
  <MyIP/>
  <Shell>sh</Shell>
  <Privileged>false</Privileged>
  <Support/>
  <Project>https://github.com/klppl/RePlex/</Project>
  <Overview>A self-hosted Spotify Wrapped for your Plex server, powered by Tautulli.</Overview>
  <Category>MediaApp:Video</Category>
  <WebUI/>
  <TemplateURL/>
  <Icon>https://raw.githubusercontent.com/klppl/RePlex/refs/heads/main/public/icon.png</Icon>
  <ExtraParams/>
  <PostArgs/>
  <CPUset/>
  <DateInstalled>1766301942</DateInstalled>
  <DonateText/>
  <DonateLink/>
  <Requires/>
  <Config Name="App path" Target="/app/prisma" Default="/mnt/user/appdata/replex" Mode="rw" Description="" Type="Path" Display="always" Required="false" Mask="false">/mnt/user/appdata/replex</Config>
  <Config Name="PORT" Target="3000" Default="" Mode="tcp" Description="" Type="Port" Display="always" Required="false" Mask="false">3000</Config>
  <Config Name="DATABASE" Target="DATABASE_URL" Default="file:/app/prisma/dev.db" Mode="" Description="" Type="Variable" Display="always" Required="false" Mask="false">file:/app/prisma/dev.db</Config>
  <Config Name="JWT SECRET" Target="JWT_SECRET" Default="CHANG3_M3_!!!" Mode="" Description="CHANGE ME!" Type="Variable" Display="always" Required="false" Mask="false">CHANG3_M3_!!!</Config>
  <Config Name="COOKIES" Target="DISABLE_SECURE_COOKIES" Default="true" Mode="" Description="Use if hosted public" Type="Variable" Display="always" Required="false" Mask="false">true</Config>
  <TailscaleStateDir/>
</Container>
